            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">wh- determiner what, which </a:documentation>
            <rng:value>WPg</rng:value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">possessive wh- pronoun whose </a:documentation>
            <rng:value>WPO</rng:value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">objective wh- pronoun whom, which, that </a:documentation>
            <rng:value>WPS</rng:value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">nominative wh- pronoun who, which, that </a:documentation>
            <rng:value>WQL</rng:value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">wh- qualifier how </a:documentation>
            <rng:value>WRB</rng:value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">wh- adverb how, where, when</a:documentation>
          </rng:choice>
        </rng:attribute>
      </rng:optional>
      <ref name="att.global.attribute.xmlid"/>
      <ref name="att.global.attribute.n"/>
      <ref name="att.global.attribute.xmllang"/>
      <ref name="att.global.attribute.xmlbase"/>
      <ref name="att.segLike.attribute.function"/>
      <ref name="att.segLike.attribute.part"/>
      <empty/>
    </element>
  </define>
  <define xmlns="http://relaxng.org/ns/structure/1.0" name="c">
    <element name="c">
      <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(character) represents a character.</a:documentation>
      <rng:ref name="macro.xtext"/>
      <ref name="att.global.attribute.xmlid"/>
      <ref name="att.global.attribute.n"/>
      <ref name="att.global.attribute.xmllang"/>
      <ref name="att.global.attribute.xmlbase"/>
      <ref name="att.segLike.attribute.type"/>
      <ref name="att.segLike.attribute.function"/>
      <ref name="att.segLike.attribute.part"/>
      <empty/>
    </element>
  </define>
  <define xmlns="http://relaxng.org/ns/structure/1.0" name="mw">
    <element name="mw">
      <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
      <rng:text/>
      <ref name="att.global.attribute.xmlid"/>
      <ref name="att.global.attribute.n"/>
      <ref name="att.global.attribute.xmllang"/>
      <ref name="att.global.attribute.xmlbase"/>
      <rng:optional>
        <rng:attribute name="pos">
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">sequence of POS codes associated with this
lexical item</a:documentation>
          <text/>
        </rng:attribute>
      </rng:optional>
      <rng:optional>
        <rng:attribute name="subtype">
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">contextual code indicating location of this lexical item</a:documentation>
          <rng:choice>
            <rng:value>FW</rng:value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">foreign word (hyphenated before regular
tag)</a:documentation>
            <rng:value>HL</rng:value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">word occurring in headline (hyphenated after regular tag)</a:documentation>
            <rng:value>NC</rng:value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">cited word (hyphenated after regular tag) </a:documentation>
            <rng:value>TL</rng:value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">word occurring in title (hyphenated after regular tag)</a:documentation>
          </rng:choice>
        </rng:attribute>
      </rng:optional>
      <empty/>
    </element>
  </define>
  <rng:start>
    <rng:choice>
      <ref xmlns="http://relaxng.org/ns/structure/1.0" name="TEI"/>
      <ref xmlns="http://relaxng.org/ns/structure/1.0" name="teiCorpus"/>
    </rng:choice>
  </rng:start>
</rng:grammar>
